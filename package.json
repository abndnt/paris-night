{
  "name": "flight-search-saas",
  "version": "1.0.0",
  "description": "AI-powered flight search platform with points optimization",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:simple": "node scripts/start-simple.js",
    "dev:simple:direct": "ts-node-dev --respawn --transpile-only src/server.simple.ts",
    "dev:minimal": "ts-node-dev --respawn --transpile-only src/server.dev.ts",
    "setup": "node scripts/setup-development.js",
    "setup:env": "node scripts/setup-env.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:performance": "artillery run performance/load-test.yml --output performance-report/load-test.json",
    "test:stress": "artillery run performance/stress-test.yml --output performance-report/stress-test.json",
    "test:smoke": "jest --testPathPattern=smoke --runInBand",
    "test:server": "npm run build && node dist/server.js",
    "test:apis": "node scripts/test-apis.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "flight",
    "search",
    "points",
    "travel",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^9.40.0",
    "@types/socket.io": "^3.0.1",
    "@types/stripe": "^8.0.416",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^5.10.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.8.1",
    "stripe": "^18.3.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@sentry/profiling-node": "^9.41.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "artillery": "^2.0.23",
    "axios": "^1.6.2",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "playwright": "^1.54.1",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "validator": "^13.15.15",
    "xss": "^1.0.15"
  }
}
